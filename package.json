{
  "name": "cydiarepo",
  "version": "1.1.6",
  "description": "Cydia repo for Favna",
  "repository": "git@github.com:favna/cydia.git",
  "author": "Jeroen Claassens <support@favware.tech>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "cz": "git-cz",
    "up": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@favware/ts-config": "^2.0.0",
    "@types/node": "^14.0.5",
    "cz-conventional-changelog": "3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "standard-version": "^8.0.0",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
